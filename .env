# -------------------------------------------
#                                           |
#            适合所有环境的通用配置           |
#                                           |
# -------------------------------------------

# 产品名称
VITE_APP_NAME=blog

# 网站首页标题
VITE_APP_TITLE=个人博客

# 网站描述
VITE_APP_DESC=分享个人学习记录的个人博客

# 网站关键词
VITE_APP_KEYWORDS=个人博客,前端,后端,全栈,学习,分享

# 管理系统的默认主题（可选 light 明亮， dark 暗黑）
VITE_APP_DEFAULT_THEME=light

# 开发环境路由历史模式（Hash模式传"hash"、HTML5模式传"h5"、Hash模式带base参数传"hash,base参数"、HTML5模式带base参数传"h5,base参数"）
# 使用h5路由模式时， 直接访问 /admin/home 会失效，需要 /admin 跳转到 /admin/home
VITE_ROUTER_HISTORY = "hash"

# 构建输出目录
VITE_BUILD_OUTPUT_DIR = "blog"

# mock 代理地址
VITE_MOCK_PROXY_URL = "http://localhost:3000"

# 项目部署目录路径
# 参考 https://cn.vitejs.dev/config/shared-options.html#base
VITE_BASE_URL=/blog/

# iconify图标作为组件的前缀
VITE_ICON_PREFIX=icon

# 本地SVG图标作为组件的前缀, 请注意一定要包含 VITE_ICON_PREFIX
# 格式 {VITE_ICON_PREFIX}-{本地图标集合名称}
VITE_ICON_LOCAL_PREFIX=icon-local

VITE_DIST_NAME=blog

VITE_HTTP_PROXY=Y

VITE_VISUALIZER=Y

VITE_COMPRESS=Y

# gzip | brotliCompress | deflate | deflateRaw
VITE_COMPRESS_TYPE=gzip
